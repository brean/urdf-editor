{
	"name": "urdf-editor",
	"version": "0.0.1",
	"author": "Andreas Bresser <self@andreasbresser.de>",
	"license": "BSD-3-Clause",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run smui && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"smui": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"dependencies": {
		"@monaco-editor/loader": "^1.5.0",
		"@smui/button": "^8.0.0-beta.3",
		"@smui/card": "^8.0.0-beta.3",
		"@smui/dialog": "^8.0.0-beta.3",
		"@smui/drawer": "^8.0.0-beta.3",
		"@smui/fab": "^8.0.0-beta.3",
		"@smui/form-field": "^8.0.0-beta.3",
		"@smui/icon-button": "^8.0.0-beta.3",
		"@smui/layout-grid": "^8.0.0-beta.3",
		"@smui/list": "^8.0.0-beta.3",
		"@smui/menu": "^8.0.0-beta.3",
		"@smui/menu-surface": "^8.0.0-beta.3",
		"@smui/select": "^8.0.0-beta.3",
		"@smui/slider": "^8.0.0-beta.3",
		"@smui/switch": "^8.0.0-beta.3",
		"@smui/textfield": "^8.0.0-beta.3",
		"@smui/top-app-bar": "^8.0.0-beta.3",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@threlte/core": "^8.0.1",
		"@threlte/extras": "^9.0.1",
		"@types/three": "^0.173.0",
		"monaco-editor": "^0.52.2",
		"smui-theme": "^8.0.0-beta.3",
		"svelte": "^5.0.0",
		"svelte-splitpanes": "^8.0.9",
		"three": "^0.173.0",
		"urdf-viewer": "^0.2.7"
	},
	"type": "module",
	"devDependencies": {
		"@sveltejs/kit": "^2.16.0",
		"svelte-check": "^4.0.4",
		"svelte-sequential-preprocessor": "^2.0.2",
		"typescript": "^5.6.3",
		"vite": "^6.0.0"
	}
}
